{  
  "name": "f2e-server",  
  "manifest_version": 2, 
  "version": "0.0.1", 
  "icons": { "128": "f2e.png", "48": "f2e.png", "16": "f2e.png"},
  "description": "utils created in brower for f2e-server.",  
  "permissions": [
    "tabs",
    "contextMenus",
    "windows",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action":{
    "default_title": "f2e-server",
    "default_icon": "f2e.png",
    "default_popup": "popup.html"
  },
  "background" : {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at" : "document_end",
      "all_frames": true,
      "js": ["css-sprite.js"]
    }
  ],
  "web_accessible_resources": [
      "style.css"
  ]
} 